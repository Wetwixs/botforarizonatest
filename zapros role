			await ctx.author.remove_roles(role_smils)
			await ctx.author.remove_roles(role_smisf)
			await ctx.author.remove_roles(role_smilv)
			await ctx.author.remove_roles(role_ash)
			await ctx.author.remove_roles(role_yk)
			await ctx.author.remove_roles(role_lcn)
