@client.command()
async def un_role(ctx):
	unfrac_role = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Больницы ЛС')
	unfrac_role1 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Больницы СФ')
	unfrac_role2 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Больницы ЛВ')
	unfrac_role3 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник СМИ ЛС')
	unfrac_role4 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник СМИ СФ')
	unfrac_role5 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник СМИ ЛВ')
	unfrac_role6 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Автошколы')
	unfrac_role7 = discord.utils.get(ctx.message.guild.roles, name = 'Член мафии Yakuza')
	unfrac_role8 = discord.utils.get(ctx.message.guild.roles, name = 'Член мафии La Cosa Nostra')
	unfrac_role9 = discord.utils.get(ctx.message.guild.roles, name = 'Член мафии Warlock MC')
	unfrac_role10 = discord.utils.get(ctx.message.guild.roles, name = 'Член Русской Мафии')
	unfrac_role11 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Тюрьмы')
	unfrac_role12 = discord.utils.get(ctx.message.guild.roles, name = 'Военнослужащий Армии СФ')
	unfrac_role13 = discord.utils.get(ctx.message.guild.roles, name = 'Военнослужащий Армии ЛС')
	unfrac_role14 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Центрального банка')
	unfrac_role15 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Правительства')
	unfrac_role16 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Окружной Полиции')
	unfrac_role17 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Полиции ЛВ')
	unfrac_role18 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Полиции СФ')
	unfrac_role19 = discord.utils.get(ctx.message.guild.roles, name = 'Сотрудник Полиции ЛС')
	unfrac_role20 = discord.utils.get(ctx.message.guild.roles, name = 'Агент ФБР')
	unfrac_role21 = discord.utils.get(ctx.message.guild.roles, name = 'Старший Агент ФБР')
	await ctx.author.remove_roles(unfrac_role)
	await ctx.author.remove_roles(unfrac_role1)
	await ctx.author.remove_roles(unfrac_role2)
	await ctx.author.remove_roles(unfrac_role3)
	await ctx.author.remove_roles(unfrac_role4)
	await ctx.author.remove_roles(unfrac_role5)
	await ctx.author.remove_roles(unfrac_role6)
	await ctx.author.remove_roles(unfrac_role7)
	await ctx.author.remove_roles(unfrac_role8)
	await ctx.author.remove_roles(unfrac_role9)
	await ctx.author.remove_roles(unfrac_role10)
	await ctx.author.remove_roles(unfrac_role11)
	await ctx.author.remove_roles(unfrac_role12)
	await ctx.author.remove_roles(unfrac_role13)
	await ctx.author.remove_roles(unfrac_role14)
	await ctx.author.remove_roles(unfrac_role15)
	await ctx.author.remove_roles(unfrac_role16)
	await ctx.author.remove_roles(unfrac_role17)
	await ctx.author.remove_roles(unfrac_role18)
	await ctx.author.remove_roles(unfrac_role19)
	await ctx.author.remove_roles(unfrac_role20)
	await ctx.author.remove_roles(unfrac_role21)
	await ctx.send(embed=discord.Embed(description=f"{ctx.message.author.mention} `все роли фракций были успешно сняты!`", color = random.choice(colors)))
