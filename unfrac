	    unfrac_role21 = discord.utils.get(ctx.message.guild.roles, name = 'Старший Агент ФБР')


	    emb = discord.Embed(title = 'Снятие ролей', description=f'Роль снята с пользователя {member.mention}', color = random.choice(colors))
	    emb.set_footer(text="Тех. Администратор Discord - Prophet Romanov", icon_url="https://i.imgur.com/Ul8dcJ2.jpg")
	    message = await ctx.send(embed = emb)
